<h1>All Objects</h1>
@if(@Model.Count == 0)
{
  <h3>You have not added any Objects</h3>
}
@foreach(var object in Model)
{
  <ul>
    <li><a href="/object/@object.GetId()">@object.GetName()</a> | <a href="/object/edit/@object.GetId()">Edit</a> | <a href="/object/delete/@object.GetId()">Delete</a></li>
  </ul>
}
<p><a href="/object/new">Add a new Object</a></p>
<p><a href="/">Return Home</a></p>
